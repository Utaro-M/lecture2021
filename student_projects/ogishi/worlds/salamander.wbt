DEF GOALs Solid {
  translation 0 0.1 0
  rotation 0 1 0 1.570796
  children [
    DEF GOALS_GROUP Group {
      children [
        Transform {
          translation 0 0 -1
          children [
            DEF GOAL_SHAPE1 Shape {
              appearance DEF GOAL_APPEARANCE PBRAppearance {
                baseColor 1 0 0
                roughness 1
                metalness 0
              }
              geometry Box {
                size 12 0.7 0.02
              }
            }
          ]
        }
        Transform {
          translation 0 0 1
          children [
            DEF GOAL_SHAPE2 Shape {
              appearance DEF GOAL_APPEARANCE2 PBRAppearance {
                baseColor 0 0 1
                roughness 1
                metalness 0
              }
              geometry Box {
                size 12 0.7 0.02
              }
            }
          ]
        }
      ]
    }
  ]
  name "goal"
  //boundingObject USE SIDES_GROUP
  locked TRUE
}

DEF BALL Solid {
  translation 0 0.5 0
  rotation 0.7071067811865477 0 0.7071067811865477 2.62318
  children [
    DEF S Shape {
      appearance DEF LIGHT_BROWN PBRAppearance {
        baseColor 0.807843 0.72549 0.498039
        roughness 1.1102230246251565e-16
        metalness 0
      }
      geometry Sphere {
        radius 0.1
        subdivision 2
      }
    }
  ]
  name "ball"
  immersionProperties [
    DEF IMMERSION_HOGE ImmersionProperties {
      fluidName "fluid"
      referenceArea "xyz-projection"
      dragForceCoefficients 0.1 0 0
      dragTorqueCoefficients 0.1 0 0
      viscousResistanceTorqueCoefficient 0.5
    }
  ]
  boundingObject USE S
  physics Physics {
    mass 1
    damping Damping {
      linear 0.5
      angular 0.5
    }
  }
}


DEF AQUARIUM Solid {
  translation 0 0.1 0
  children [
    DEF SIDES_GROUP Group {
      children [
        Transform {
          translation 0 0 4
          children [
            DEF GLASS_SHAPE1 Shape {
              appearance DEF GLASS_APPEARANCE PBRAppearance {
                baseColor 1 0.6 0.6
                roughness 1
                metalness 0
              }
              geometry Box {
                size 12 0.7 0.02
              }
            }
          ]
        }
        Transform {
          translation 0 0 -4
          children [
            USE GLASS_SHAPE1
          ]
        }
        Transform {
          translation -6 0 0
          rotation 0 1 0 1.5708
          children [
            DEF GLASS_SHAPE2 Shape {
              appearance USE GLASS_APPEARANCE
              geometry Box {
                size 8 0.7 0.02
              }
            }
          ]
        }
        Transform {
          translation 6 0 0
          rotation 0 1 0 1.5708
          children [
            USE GLASS_SHAPE2
          ]
        }
      ]
    }
  ]
  name "aquarium"
  boundingObject USE SIDES_GROUP
  locked TRUE
}
DEF WATER Fluid {
